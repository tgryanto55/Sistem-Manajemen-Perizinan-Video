{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tailwindcss -i ./public/assets/css/input.css -o ./public/assets/css/app.css --watch & esbuild ./public/assets/js/app.js --bundle --outfile=./public/assets/js/bundle.js --watch",
    "build": "tailwindcss -i ./public/assets/css/input.css -o ./public/assets/css/app.css --minify && esbuild ./public/assets/js/app.js --bundle --outfile=./public/assets/js/bundle.js --minify"
  },
  "dependencies": {
    "@imacrayon/alpine-ajax": "^0.12.6",
    "@tailwindcss/cli": "^4.0.0",
    "alpinejs": "^3.15.8",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "esbuild": "^0.27.3"
  }
}
